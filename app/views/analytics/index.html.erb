
<strong>Projects Per Customer</strong>

<div class ="toggle bar-field"> 
	<%= bar :bar_chart, @barSize, @bar_data_2.to_json %>
</div>

<div class ="toggle table-field"> 
	<table>
		<tr>
			<th> Project</th>
			<th> Users </th>
		</tr>
		<% @project_ids.each do |pi| %>
		<tr>
			<td><%= Project.find(pi).name %></td>
			<td><%=  %></td>
		</tr>
		<% end %>
	</table>
</div>

<table>
	<tr>
		<th> Project Name</th>
		<th> User </th>
		<th> Last Logged In </th>
		<th> Sign in Count </th>
	</tr>
	<%@project_names.each do |pname| %><%@user_name.each do |uname| %> 
	<tr>
		<td><%=  pname %></td>
		<td><%=  uname.first_name %></td>
		<td><%=  uname.last_sign_in_at %></td>
		<td><%=  uname.sign_in_count %></td>

	</tr>
	<% end %> <% end %>

</table>

